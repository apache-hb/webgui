if host_machine.system() == 'windows' and get_option('default_library') == 'shared'
    cpp_args = ['-DAWS_ROUTE53_EXPORTS=1']
else
    cpp_args = []
endif

libaws_cpp_sdk_route53 = library(
    'aws-cpp-sdk-route53',
    src,
    include_directories: inc,
    cpp_args: cpp_args + public_cpp_args,
    dependencies: [aws_cpp_sdk_core_dep],
    install: true,
    version: meson.project_version(),
)

aws_cpp_sdk_route53_dep = declare_dependency(
    include_directories: inc,
    link_with: libaws_cpp_sdk_route53,
    dependencies: [aws_cpp_sdk_core_dep],
    compile_args: public_cpp_args,
)

meson.override_dependency('aws-cpp-sdk-route53', aws_cpp_sdk_route53_dep)

pkgs += {
    'aws-cpp-sdk-route53': {
        'lib': libaws_cpp_sdk_route53,
        'extra_cflags': public_cpp_args,
    }
}
